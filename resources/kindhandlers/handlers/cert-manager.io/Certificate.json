{
  "sectionName": "CertMananger",
  "kindSectionName": "Certificates",
  "helpLink": "https://cert-manager.io/docs/concepts/certificate/",
  "refMappers":
  [
    {
      "source": {
        "pathParts": ["spec", "secretName"],
        "namespaceRef": "Implicit"
      },
      "type": "name",
      "target": {
        "kind": "Secret"
      }
    },
    {
      "source": {
        "pathParts": [
          "spec", "issuerRef", "name"
        ]
      },
      "type": "name",
      "target": {
        "kind": "$(Issuer|ClusterIssuer)"
      }
    }
 ]
}
