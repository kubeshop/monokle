{
  "name": "manifestui",
  "version": "0.0.1",
  "author": "Kubeshop",
  "description": "UI for managing k8s manifests",
  "homepage": "./",
  "main": "build/electron/main.js",
  "contributors": [
    "ole@kubeshop.io",
    "steen@kubeshop.io"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kubeshop/manifestui.git"
  },
  "private": true,
  "devDependencies": {
    "@rescripts/cli": "^0.0.16",
    "@rescripts/rescript-env": "^0.0.14",
    "@rescripts/utilities": "^0.0.8",
    "@types/dagre": "^0.7.44",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "electron": "^13.0.1",
    "electron-builder": "^22.11.7",
    "prettier": "2.3.0",
    "wait-on": "^5.3.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "@rjsf/bootstrap-4": "^2.5.1",
    "@rjsf/core": "^2.5.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.23",
    "@types/micromatch": "^4.0.1",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/redux-logger": "^3.0.8",
    "dagre": "^0.8.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^1.5.0",
    "es6-tween": "^5.5.11",
    "jsonpath-plus": "^5.0.7",
    "loglevel": "^1.7.1",
    "micromatch": "^4.0.4",
    "monaco-editor-webpack-plugin": "^4.0.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.0",
    "react-dom": "^17.0.2",
    "react-flow-renderer": "^9.6.0",
    "react-folder-tree": "5.0.3",
    "react-monaco-editor": "0.43.0",
    "react-redux": "^7.2.4",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.3.2",
    "web-vitals": "^1.1.2",
    "yaml": "2.0.0-5"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn run build && tsc -p electron && electron-builder",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "io.kubeshop.manifestui",
    "productName": "ManifestUI",
    "copyright": "Copyright Â© 2021 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "extraResources": [
      {
        "from": "resources",
        "to": "resources"
      }
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
