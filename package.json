  "scripts": {
    "start": "craco start",
    "dev": "npm run electron:dev",
    "build": "craco --max_old_space_size=4096 build",
    "test": "craco test --watchAll=false",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run start\" \"node ./scripts/wait.js && tsc -p electron -w\" \"node ./scripts/wait.js && tsc -p electron && cross-env NODE_ENV=development electron .\"",
    "electron:debug": "concurrently \"cross-env BROWSER=none npm run start\" \"node ./scripts/wait.js && tsc -p electron -w\" \"node ./scripts/wait.js && tsc -p electron && cross-env NODE_ENV=development electron --remote-debugging-port=9223 .\"",
    "electron:build": "npm run build && tsc -p electron && electron-builder --publish \"never\"",
    "electron:build:mac": "npm run build && tsc -p electron && electron-builder --publish \"never\" && (ls -alh dist | grep Monokle && du -sh dist/mac || true)",
    "electron:build:ci": "npm run build && tsc -p electron",
    "lint": "npm run lint:config && npm run lint:src ",
    "stylelint": "stylelint  \"./src/**/*.tsx\"",
    "lint:config": "eslint .",
    "lint:src": "eslint \"{src,electron,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepush": "npm run lint",
    "format:all": "prettier --write \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "ui-test": "xvfb-maybe npx playwright test --workers=1",
    "custom-script": "npx ts-node src/scripts/index.ts",
    "sign-dev-electron-app": "codesign --deep --force --verbose --sign - node_modules/electron/dist/Electron.app"
  },
  "build": {
    "appId": "io.kubeshop.monokle",
    "productName": "Monokle",
    "copyright": "Copyright Â© 2021 ${author}",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "build/icon.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./electron/entitlements.mac.inherit.plist",
      "entitlementsInherit": "./electron/entitlements.mac.inherit.plist",
      "target": [
        {
          "target": "default",
          "arch": [
            "universal"
          ]
        }
      ]
    },
    "artifactName": "${productName}-${os}-${version}-${arch}.${ext}",
    "afterSign": "./notarization/afterSignHook.js",
    "dmg": {
      "icon": "build/icon.png",
      "writeUpdateInfo": true
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico",
      "sign": "./sign/customSign.js",
      "publisherName": [
        "Kubernetes Innovation Labs, LLC",
        "KUBERNETES INNOVATION LABS, LLC"
      ]
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico",
      "differentialPackage": true,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": true,
      "createDesktopShortcut": false,
      "menuCategory": "Kubeshop",
      "shortcutName": "Monokle"
    },
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "build/icon.png",
      "category": "Development",
      "maintainer": "Kubeshop",
      "vendor": "Kubeshop",
      "executableName": "Monokle",
      "synopsis": "Your kubernetes best friend!",
      "description": "A free, open-source tool, to help you navigate kubernetes development. By Kubeshop."
    },
    "appImage": {
      "artifactName": "${productName}-${os}-${version}-${arch}.${ext}"
    },
    "deb": {
      "packageCategory": "Kubeshop",
      "priority": "standard"
    },
    "rpm": {
      "packageCategory": "Kubeshop"
    },
    "extends": null,
    "files": [
      "build/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!**/node_modules/eslint*/**/*",
      "!**/node_modules/@testing-library/**/*",
      "!**/node_modules/@typescript-eslint/**/*",
      "!**/node_modules/typescript/**/*",
      "!**/node_modules/*webpack*/**/*",
      "!**/node_modules/less/**/*",
      "!**/node_modules/babel-runtime/**/*",
      "!**/node_modules/yaml-language-server/lib/umd/**/*",
      "!**/node_modules/antd/es/**/*",
      "!**/node_modules/@jest/**/*",
      "!**/node_modules/monaco-editor/min/**/*",
      "!**/node_modules/monaco-editor/min-maps/**/*",
      "!**/docs/**/*",
      "!notarization/**/*"
    ],
    "extraResources": [
      {
        "from": "resources",
        "to": "resources"
      },
      {
        "from": "resources/darwin",
        "to": "app"
      },
      {
        "from": "resources",
        "to": "app"
      }
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
