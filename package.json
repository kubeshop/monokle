{
  "name": "monokle",
  "version": "0.0.5",
  "author": "Kubeshop",
  "description": "UI for managing k8s manifests",
  "homepage": "./",
  "main": "build/electron/main.js",
  "contributors": [
    "ole@kubeshop.io",
    "steen@kubeshop.io",
    "andrei@kubeshop.io",
    "catalin@kubeshop.io",
    "thomas@kubeshop.io",
    "sierra@kubeshop.io"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kubeshop/monokle.git"
  },
  "private": true,
  "devDependencies": {
    "@craco/craco": "6.2.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/dagre": "^0.7.46",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.24",
    "@types/styled-components": "^5.1.12",
    "@types/uuid": "^8.3.1",
    "concurrently": "^6.2.0",
    "craco-alias": "^3.0.1",
    "craco-less": "^1.18.0",
    "cross-env": "^7.0.3",
    "electron": "^13.1.8",
    "electron-builder": "^22.11.11",
    "electron-reload": "^1.5.0",
    "eslint-config-airbnb": "~18.2.1",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-jsx-a11y": "~6.4.1",
    "eslint-plugin-prettier": "~3.4.0",
    "eslint-plugin-react": "~7.24.0",
    "eslint-plugin-react-hooks": "~4.2.0",
    "eslint-plugin-unused-imports": "^1.1.2",
    "file-loader": "^6.2.0",
    "husky": "^7.0.1",
    "monaco-editor-webpack-plugin": "^4.1.1",
    "prettier": "2.3.2",
    "react-scripts": "4.0.3",
    "redux-devtools-extension": "^2.13.9",
    "typescript": "^4.3.5",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "4.6.2",
    "@kubernetes/client-node": "^0.15.0",
    "@rjsf/antd": "^3.0.0",
    "@rjsf/core": "^3.0.0",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^16.4.12",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/redux-logger": "^3.0.9",
    "antd": "4.16.10",
    "chokidar": "^3.5.2",
    "dagre": "^0.8.5",
    "dayjs": "^1.10.6",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.0",
    "es6-tween": "^5.5.11",
    "fast-deep-equal": "^3.1.3",
    "jsonpath-plus": "^6.0.1",
    "loglevel": "^1.7.1",
    "micromatch": "^4.0.4",
    "monaco-yaml": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-flow-renderer": "^9.6.6",
    "react-hotkeys-hook": "^3.4.0",
    "react-monaco-editor": "0.44.0",
    "react-redux": "^7.2.4",
    "react-use": "17.2.4",
    "redux": "^4.1.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "shell-path": "^2.1.0",
    "styled-components": "5.3.0",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0",
    "worker-loader": "^3.0.8",
    "yaml": "2.0.0-6"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --watchAll=false",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "npm run build && tsc -p electron && electron-builder",
    "electron:build:mac": "npm run build && tsc -p electron && electron-builder && (ls -alh dist | grep Monokle && du -sh dist/mac || true)",
    "eject": "react-scripts eject",
    "lint": "npm run lint:config && npm run lint:src ",
    "lint:config": "eslint .",
    "lint:src": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "lintfix": "eslint --fix \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "precommit": "npm run lint && npm test",
    "format:all": "prettier --write \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install"
  },
  "build": {
    "appId": "io.kubeshop.monokle",
    "productName": "Monokle",
    "copyright": "Copyright Â© 2021 ${author}",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "build/icon.png"
    },
    "dmg": {
      "icon": "build/icon.png"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico"
    },
    "extends": null,
    "files": [
      "build/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!**/node_modules/eslint*/**/*",
      "!**/node_modules/@testing-library/**/*",
      "!**/node_modules/@typescript-eslint/**/*",
      "!**/node_modules/typescript/**/*",
      "!**/node_modules/*webpack*/**/*",
      "!**/node_modules/less/**/*",
      "!**/node_modules/babel-runtime/**/*",
      "!**/node_modules/yaml-language-server/lib/umd/**/*",
      "!**/node_modules/antd/es/**/*",
      "!**/node_modules/@babel/**/*",
      "!**/node_modules/@jest/**/*",
      "!**/node_modules/monaco-editor/min/**/*",
      "!**/node_modules/monaco-editor/min-maps/**/*"
    ],
    "extraResources": [
      {
        "from": "resources",
        "to": "resources"
      }
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "lint-staged": {
    "\"{src,test}/**/*.{js,jsx,ts,tsx}\"": [
      "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\" --fix",
      "git add"
    ],
    ".": [
      "eslint . --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "@models/(.+)": "<rootDir>/src/models/$1",
      "@redux/(.+)": "<rootDir>/src/redux/$1",
      "@src/(.+)": "<rootDir>/src/$1"
    }
  }
}
